{
  "name": "chinese-quick-mapping-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier --check .",
    "fetch:texts": "node scripts/fetch_texts.mjs",
    "build:snippets": "node scripts/build_snippets.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "e2e": "cypress run",
    "cy:open": "cypress open",
    "format": "prettier --write .",
    "build:with-tests": "npm run e2e && npm run build",
    "deploy": "npm run build && npx wrangler deploy"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/ramda": "^0.28.20",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.2",
    "axios": "^1.1.3",
    "cypress": "^12.17.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.8",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "ramda": "^0.28.0",
    "scrape-brrr": "^1.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.0",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^5.4.4",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18.13"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.37.0"
  }
}